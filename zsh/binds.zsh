# bindkey -v
bindkey '^[[H'    beginning-of-line # C-a
bindkey '^[[F'    end-of-line # C-e
bindkey '^[[1;5D' backward-word # C-b
bindkey '^[[1;5C' forward-word # C-f
bindkey '^H'      backward-delete-word # C-w
bindkey '^[[3~'   delete-char  # C-h
bindkey '^[w'     kill-word # A-w
bindkey '^[[3;5~' kill-word # A-d
